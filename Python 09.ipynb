    "class Employee:\n",
    "    def __init__(self, name, salary):\n",
    "        self.name = name\n",
    "        self.__salary = salary\n",
    "\n",
    "    def get_salary(self):\n",
    "        return self.__salary\n",
    "\n",
    "\n",
    "emp = Employee(\"Amit\", 40000)\n",
    "print(\"Salary:\", emp.get_salary())"
